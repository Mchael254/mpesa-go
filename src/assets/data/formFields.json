        {
          "groupId": "prime_identity",
          "subGroupId": "",
          "fieldId": "idNumber",
          "type": "text",
          "label": {
            "en": "National ID Number",
            "ke": "Namba ya Kitambulisho cha Kitaifa",
            "fr": "Num'ro de Carte Nationale d'Identit'"
          },
          "dynamicLabel": {
            "field": "modeOfIdentityId",
            "mapping": {
              "NATIONAL_ID": {
                "en": "National ID Number",
                "ke": "Namba ya Kitambulisho cha Kitaifa",
                "fr": "Num'ro de Carte Nationale d'Identit'"
              },
              "PASSPORT_NUMBER": {
                "en": "Passport Number",
                "ke": "Namba ya Pasipoti",
                "fr": "Num'ro de Passeport"
              },
              "BIRTH_CERT_NUMBER": {
                "en": "Birth Certificate Number",
                "ke": "Namba ya Cheti cha Kuzaliwa",
                "fr": "Num'ro de Certificat de Naissance"
              },
              "CERT_OF_INCOP_NUMBER": {
                "en": "Certificate of Incorporation Number",
                "ke": "Namba ya Cheti cha Usajili",
                "fr": "Num'ro de Certificat d'Incorporation"
              },
              "TAX_NUMBER": {
                "en": "Tax Number",
                "ke": "Namba ya Ushuru",
                "fr": "Num'ro Fiscal"
              }
            }
          },
          "defaultValue": null,
          "visible": true,
          "disabled": false,
          "validations": [],
          "conditions": [
            {
              "field": "modeOfIdentityId",
              "value": "NATIONAL_ID",
              "config": {
                "validations": [
                  {
                    "type": "pattern",
                    "value": "^[0-9]{6,10}$",
                    "message": {
                      "en": "National ID must be 6 to 10 digits, e.g. 34776252",
                      "ke": "Kitambulisho lazima kiwe na tarakimu 6 hadi 10, mfano 34776252",
                      "fr": "La carte d'identité doit comporter de 6 à 10 chiffres, ex. 34776252"
                    }
                  }
                ]
              }
            },
            {
              "field": "modeOfIdentityId",
              "value": "PASSPORT_NUMBER",
              "config": {
                "validations": [
                  {
                    "type": "pattern",
                    "value": "^[A-Z0-9]{6,9}$",
                    "message": {
                      "en": "Passport number must be 6 to 9 alphanumeric characters",
                      "ke": "Nambari ya pasipoti iwe na herufi na/au tarakimu kati ya 6 hadi 9",
                      "fr": "Le numéro de passeport doit contenir de 6 à 9 caractères alphanumériques"
                    }
                  }
                ]
              }
            },
            {
              "field": "modeOfIdentityId",
              "value": "BIRTH_CERT_NUMBER",
              "config": {
                "validations": [
                  {
                    "type": "pattern",
                    "value": "^[A-Z0-9-/]{6,15}$",
                    "message": {
                      "en": "Birth Certificate number must be 6 to 15 characters (letters, numbers, or '/')",
                      "ke": "Nambari ya cheti cha kuzaliwa iwe na herufi, tarakimu au / kati ya 6 hadi 15",
                      "fr": "Le numéro d'acte de naissance doit comporter de 6 à 15 caractères (lettres, chiffres ou '/')"
                    }
                  }
                ]
              }
            },
            {
              "field": "modeOfIdentityId",
              "value": "CERT_OF_INCOP_NUMBER",
              "config": {
                "validations": [
                  {
                    "type": "pattern",
                    "value": "^[A-Z0-9-/]{6,20}$",
                    "message": {
                      "en": "Certificate of Incorporation number must be 6 to 20 characters",
                      "ke": "Nambari ya cheti cha usajili iwe kati ya herufi/tarakimu 6 hadi 20",
                      "fr": "Le numéro du certificat d'incorporation doit comporter de 6 à 20 caractères"
                    }
                  }
                ]
              }
            }
          ],
