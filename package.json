{
  "name": "turnquestv6-fe",
  "version": "6.2.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --proxy-config proxy.conf.json",
    "start:watch": "ng serve --watch  --proxy-config proxy.conf.json",
    "build": "ng build --base-href=/TQV6/ --source-map",
    "build:prod": "npm run build --prod --aot",
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage",
    "explore": "source-map-explorer dist/**/main.*.js --no-border-checks",
    "docs": "compodoc -p tsconfig.json -s -n \"TurnQuest Version 6 App documentation\" -d ./documentation --theme vagrant --disableSourceCode --disableTemplateTab --disableStyleTab -r --port 8085 --open --silent --hideGenerator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.1.5",
    "@angular/common": "^16.1.0",
